  "repository": {
    "type": "git",
    "url": "https://github.com/angelxaces/ci_bootstrap_3.git"
  },
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-css": "node-sass -w src/sass/frontend.scss -o assets/dist/css --source-map true --source-map-contents scss --output-style compressed --follow",
    "build-server": "babel -d ./build ./server -s",
    "build": "npm run build-css && npm run build-server",
    "transpile": "babel src/js -d assets/dist/js --presets=es2015,react",
    "lint": "eslint source/ --quiet",
    "start": "node ./build/index.js",
    "start2": "webpack-dev-server --hot",
    "debug": "node --debug ./build/index.js",
    "test": "mocha --compilers js:babel-core/register App.jsx",
    "validate": "npm run lint; npm run test && npm outdated --depth 0",
    "babel-opt": "babel src/js/script.js --out-file assets/dist/js/script.js --source-maps --presets=es2015,react"
  },
  "dependencies": {
    "babel-cli": "^6.24.0",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-native": "^1.9.1",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "git+https://github.com/twbs/bootstrap.git",
    "bootstrap-grid-react": "^2.0.1",
    "bootstrap-sass": "^3.3.7",
    "caseless": "^0.12.0",
    "debug": "^2.6.8",
    "grid": "^3.3.6",
    "har-validator": "^5.0.3",
    "jsx": "^0.9.89",
    "mime-db": "^1.28.0",
    "qs": "^6.4.0",
    "rc": "^1.2.1",
    "react": "^15.5.4",
    "react-addons-shallow-compare": "^15.4.2",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.4.2",
    "react-grid-system": "^2.9.1",
    "require-dir": "*",
    "rimraf": "^2.6.1",
    "string_decoder": "^1.0.1",
    "uuid": "^3.0.1",
    "video.js": "^5.19.2",
    "webpack": "^2.5.1"
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "bower": "*",
    "composer": "*",
    "del": "*",
    "grunt": "^1.0.1",
    "grunt-sass": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-changed": "^3.0.0",
    "gulp-clean-css": "^3.0.4",
    "gulp-cli": "^1.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "^3.2.0",
    "gulp-notify": "^3.0.0",
    "gulp-sass": "^3.1.0",
    "gulp-sequence": "^0.4.6",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^2.1.2",
    "gulp-watch": "^4.3.11",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "react-json-view": "^1.6.1",
